{
  "name": "dToretto",
  "version": "1.0",
  "description": "Replace images to Dominic Toretto",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icons/favicon-16x16.png",
    "32": "icons/favicon-32x32.png",
    "128": "icons/favicon-128x128.png",
    "192": "icons/favicon-192x192.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.min.js",
        "dToretto.js"
      ]
    }
  ]
}